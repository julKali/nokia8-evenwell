            <CheckBox android:id="@id/cbSmdRpcrouterNtfymsg" android:layout_width="wrap_content" android:layout_height="wrap_content" />
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/str_smd_rpcrouter_ntfymsg" />
        </LinearLayout>
