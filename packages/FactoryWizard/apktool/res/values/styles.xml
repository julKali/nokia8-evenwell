<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="WelcomeTheme" parent="@android:style/Theme.DeviceDefault.NoActionBar">
        <item name="android:dropDownListViewStyle">@style/CusSpinner</item>
    </style>
    <style name="WelcomeTheme2" parent="@android:style/Theme.DeviceDefault.NoActionBar">
        <item name="android:windowBackground">@color/black</item>
        <item name="android:dropDownListViewStyle">@style/CusSpinner</item>
    </style>
    <style name="CusSpinner" parent="@android:style/Widget.ListView.DropDown">
        <item name="android:background">@drawable/spinner_default</item>
        <item name="android:divider">#80f1f1f1</item>
        <item name="android:dividerHeight">0.5dip</item>
    </style>
    <style name="NoTitleDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowNoTitle">true</item>
    </style>
</resources>
